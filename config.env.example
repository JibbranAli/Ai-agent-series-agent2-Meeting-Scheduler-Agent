# Custom AI Agent Meeting Scheduler Configuration
# Copy this file to config.env and fill in your details

# Gemini API Key (Optional - for enhanced natural language processing)
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_gemini_api_key_here

# Database Configuration
DATABASE_PATH=meeting_scheduler.db

# Default User ID for the AI agent
DEFAULT_USER_ID=default_user

# Default Agent Mode (CONSERVATIVE, BALANCED, AUTONOMOUS, LEARNING, AUTONOMOUS)
DEFAULT_AGENT_MODE=BALANCED

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
